public class EjemploA
{
	public static void main(String args[])
	{
		int nombre_variable [] = new int [6];
		nombre_variable [0] = 1;
		nombre_variable [1] = 2;
		nombre_variable [2] = 3;
		nombre_variable [3] = 4;
		nombre_variable [4] = 5;
		nombre_variable [5] = 6;

			System.out.print(nombre_variable[0]);

	}
}
